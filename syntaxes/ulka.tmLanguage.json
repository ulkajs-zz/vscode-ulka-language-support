{
  "$schema": "https://raw.githubusercontent.com/martinring/tmlanguage/master/tmlanguage.json",
  "name": "ulka",
  "patterns": [
    {
      "include": "text.html.basic"
    },
    {
      "include": "#javascript"
    }
  ],
  "repository": {
    "javascript": {
      "begin": "{%",
      "end": "%}",
      "contentName": "meta.embeded.block.javascript",
      "patterns": [
        {
          "include": "source.js"
        }
      ]
    }
  },
  "scopeName": "source.ulka"
}
