{
    "name": "ulka-language-support",
    "displayName": "Ulka Language Support",
    "description": "",
    "version": "0.0.1",
    "engines": {
        "vscode": "^1.47.0"
    },
    "categories": [
        "Programming Languages"
    ],
    "contributes": {
        "languages": [{
            "id": "ulka",
            "aliases": ["ulka", "ulka"],
